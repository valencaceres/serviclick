model donor {
  id               String    @id @default(dbgenerated("public.gen_random_uuid()")) @db.Uuid
  rut              String    @unique(map: "donor_rut_idx") @db.VarChar
  name             String    @db.VarChar
  paternallastname String    @db.VarChar
